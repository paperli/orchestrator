{
  "name": "@orchestrator/plugin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:code && npm run build:ui && npm run build:inline",
    "build:code": "esbuild src/code.ts --bundle --outfile=dist/code.js --target=es2017 --format=esm --external:@orchestrator/shared",
    "build:ui": "vite build --config vite.config.ui.ts",
    "build:inline": "node scripts/inline-ui.js",
    "dev": "npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@orchestrator/shared": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.90.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "esbuild": "^0.27.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-singlefile": "^2.3.0"
  }
}
